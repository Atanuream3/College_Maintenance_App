<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.asmita.loginactivity">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.REQUEST_IGNORE_BATTERY_OPTIMIZATIONS" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/wce"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity
            android:name=".MainActivity"
            android:label="UserLogin" />
        <activity
            android:name=".ProfileActivity"
            android:label="Student Profile" />
        <activity
            android:name=".MainProfileActivity"
            android:label="Upload" />
        <activity
            android:name=".MapsActivity"
            android:label="MapView" />
        <activity
            android:name=".call"
            android:label="@string/title_activity_call"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".MessageActivity" />
        <activity android:name=".Main2Activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".UserAdminLogin"
            android:label="Welcome" />
        <activity
            android:name=".AdminLogin"
            android:label="Admin Login" />
        <activity
            android:name=".AdminHome"
            android:label="Admin Home" />
        <activity android:name=".UserHome" />
        <activity android:name=".Networking"></activity>
    </application>

</manifest>